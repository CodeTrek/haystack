  "version": "0.2.0",
  "configurations": [
    {
      "name": "Launch Server",
      "type": "go",
      "request": "launch",
      "mode": "auto",
      "program": "${workspaceFolder}/src/main.go",
      "env": {},
      "args": [
        "--server"
      ],
      "showLog": true
    },
    {
      "name": "Attach to Process",
      "type": "go",
      "request": "attach",
      "mode": "remote",
      "processId": "${command:PickProcess}"
    }
  ]
}
